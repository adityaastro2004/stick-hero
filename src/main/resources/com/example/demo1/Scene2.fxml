      <ImageView fx:id="cherrii" fitHeight="40.0" fitWidth="50.0" layoutX="559.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true" />


      <Label fx:id="CherryCount" alignment="CENTER" contentDisplay="CENTER" layoutX="527.0" layoutY="20.0" prefHeight="26.0" prefWidth="23.0" text="4">
