      <Button fx:id="PlayerScore" layoutX="257.0" layoutY="32.0" mnemonicParsing="false" opacity="0.51" prefHeight="43.0" prefWidth="81.0" style="-fx-background-radius: 10;" text="0" textFill="#ac8686">
